{
  "name": "backend",
  "version": "1.0.0",
  "main": "dist/server.js",
  "scripts": {
    "dev": "nodemon src/server.ts",
    "build": "tsc",
    "start": "npm run generate && npm run migrate:deploy && node dist/server.js",
    "migrate:dev": "prisma migrate dev",
    "migrate:deploy": "prisma migrate deploy",
    "generate": "prisma generate",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "@prisma/client": "5.10.2",
    "cors": "2.8.5",
    "dotenv": "16.4.1",
    "express": "4.18.2",
    "socket.io": "4.7.4",
    "zod": "3.22.4"
  },
  "devDependencies": {
    "@types/cors": "2.8.17",
    "@types/express": "4.17.21",
    "@types/node": "20.11.16",
    "@types/socket.io": "3.0.2",
    "nodemon": "3.0.3",
    "prisma": "5.10.2",
    "ts-node": "10.9.2",
    "typescript": "5.3.3"
  }
}
